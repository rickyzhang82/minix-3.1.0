gen uxx* {ind_reg2, %a}
    killreg %a

pat lil ngi sil $1==$3 && $2==2		call liluxxsil("neg")
pat lil com sil $1==$3 && $2==2		call liluxxsil("not")
pat lil dec sil $1==$3 			call liluxxsil("dec")
pat lil inc sil $1==$3 			call liluxxsil("inc")

proc loexxxste example loe adi ste
